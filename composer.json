{
    "name": "pderas/laravel-two-factor",
    "version": "1.0.0",
    "description": "A two factor authentication package for laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pderas\\TwoFactor\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Brock Roadhouse",
            "email": "brock.roadhouse@fabledsolutions.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pderas\\TwoFactor\\TwoFactorServiceProvider"
            ]
        }
    }
